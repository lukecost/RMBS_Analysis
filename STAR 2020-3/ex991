    <table cellspacing="0" cellpadding="0" id="z9fb32b1696fa4a8ebfe2b35ca8ddf0c7" class="DSPFListTable" style="font-family: 'Times New Roman', Times, serif; font-size: 10pt; width: 100%; text-align: left; color: #000000;">

        <tr>
          <td style="width: 18pt;"><br>
          </td>
          <td style="width: 18pt; vertical-align: top; align: right; color: #000000;">o</td>
          <td style="width: auto; vertical-align: top; text-align: justify;">
            <div style="color: #000000;">Properties subject to existing oil or gas leases</div>
          </td>
        </tr>

    </table>
